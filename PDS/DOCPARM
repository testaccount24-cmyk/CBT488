           SELECT PARM01               ASSIGN TO PARM01
                                       ORGANIZATION IS SEQUENTIAL
                                       ACCESS MODE IS SEQUENTIAL
                                       FILE STATUS IS WS-STAT.

       FD  PARM01
           RECORDING MODE F.
       01  PARM01-RECORD.
           05  PARM01-DOCU             PIC  X.
           05  FILLER                  PIC  X.
           05  PARM01-FACILITY         PIC  X.
           05  FILLER                  PIC  X.
           05  PARM01-CODE             PIC  X(05).
           05  FILLER                  PIC  X.
           05  PARM01-VALUE            PIC  X(10).
           05  FILLER                  PIC  X(52).
           05  FILLER                  PIC  X(08).

           05  WS-PARM01-MAX           PIC  9(05)    VALUE ZERO.

